/** @var \ATF\Vendor\Block\ProductVendorBlock $block */
$vendors = $block->getVendors();
$count = count($vendors);
?>

<?php if($count > 0): ?>
<ul>
    <?php foreach ($vendors as $vendor):  ?>
    <li><?= $vendor->getName() ?></li>
    <?php endforeach; ?>
</ul>
<?php else: ?>

<div>No Assigned Vendor(s) Found.</div>

<?php endif; ?>

